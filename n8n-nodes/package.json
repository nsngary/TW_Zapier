{
  "name": "tw-zapier-n8n-nodes",
  "version": "1.0.0",
  "description": "台灣在地化流程自動化平台 - n8n 自定義節點",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "copyfiles -u 1 \"**/*.{png,svg}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LinePayApi.credentials.js",
      "dist/credentials/ECPayApi.credentials.js"
    ],
    "nodes": [
      "dist/taiwan-nodes/LinePay/LinePay.node.js",
      "dist/taiwan-nodes/ECPay/ECPay.node.js",
      "dist/taiwan-nodes/TaoyuanAirport/TaoyuanAirport.node.js",
      "dist/taiwan-nodes/GovOpenData/GovOpenData.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "taiwan",
    "payment",
    "automation",
    "workflow"
  ],
  "author": {
    "name": "TW Zapier Team",
    "email": "dev@tw-zapier.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tw-zapier/n8n-nodes.git"
  },
  "bugs": {
    "url": "https://github.com/tw-zapier/n8n-nodes/issues"
  },
  "homepage": "https://github.com/tw-zapier/n8n-nodes#readme"
}
